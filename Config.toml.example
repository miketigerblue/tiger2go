[nvd]
enabled        = true
poll_interval  = "1h"
page_size      = 2000
api_key        = "REDACTED_API_KEY"
# Optional filters (uncomment/set as needed)
# cpe_name       = "cpe:2.3:o:microsoft:windows_10:1607:*:*:*:*:*:*:*"
# cve_id         = "CVE-2022-XXXXX"
# cve_tag        = "disputed"
# cvss_v3_severity = "HIGH"
# no_rejected    = true

[mitre]
enabled       = true
poll_interval = "15m"    

[epss]
enabled       = true
poll_interval = "24h"
url           = "https://api.first.org/data/v1/epss"
page_size     = 5000

[kev]
enabled       = true
poll_interval = "24h"
url           = "https://www.cisa.gov/sites/default/files/feeds/known_exploited_vulnerabilities.json"

# ----------------------------------------------------------------------
# Content length limits (configurable)
# ----------------------------------------------------------------------
[max_lengths]
summary = 200000   # default max summary length before truncation
content = 500000   # default max content length before truncation
